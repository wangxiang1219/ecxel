ws3['A4']='第三条数据的ID'
ws3['B1']='原料信息'
ws4['A1']='数据ID'
ws4['A2']='第一条数据的ID'
ws4['A3']='第二条数据的ID'
ws4['A4']='第三条数据的ID'
ws4['B1']='原料信息.Material ID'
ws4['C1']='原料信息.Name'
ws4['D1']='原料信息.Specification'
ws4['E1']='原料信息.Note'
ws4['F1']='原料信息.Manufactuer/Supplier'
del wb['Sheet']
wb.save('原料信息_子表.xlsx')


