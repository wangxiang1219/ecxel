import openpyxl



wb=openpyxl.Workbook()
ws1=wb.create_sheet(index=0,title='工作表目录')
ws2=wb.create_sheet(index=1,title='数据信息')
ws3=wb.create_sheet(index=2,title='1')
ws4=wb.create_sheet(index=2,title='2')
ws1['A1']='工作表名'
ws1['A2']='1'
ws1['A3']='2'
ws1['B1']='工作表内容'
